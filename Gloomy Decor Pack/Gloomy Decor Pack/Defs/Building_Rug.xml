<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AreaRugBase">
        <defName>RugA_Small</defName>
        <label>Celtic Rug</label>
        <graphicData>
            <texPath>Things/Building/Rug/Rug1</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(3,3)</drawSize>
            <damageData>
                <rect>(0.2,0.2,0.6,0.6)</rect>
            </damageData>
        </graphicData>
        <statBases>
			<WorkToBuild>3300</WorkToBuild>
			<Beauty>22</Beauty>
			<Bulk>14</Bulk>
			<Mass>8</Mass>
		</statBases>
        <costStuffCount>135</costStuffCount>
		<size>(3,3)</size>
		<researchPrerequisites>
			<li>Carpets_A2</li>
		</researchPrerequisites>
        <description>It's a rug with Celtic designs.</description>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <rotatable>false</rotatable>
        <saveCompressible>false</saveCompressible>
    </ThingDef>

