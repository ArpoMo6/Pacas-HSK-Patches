<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>SeedsPlease</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs</xpath>
                    <value>
                        <RoyalTitlePermitDef ParentName="DropResourcesPlus">
                            <defName>SeedsPotatoDrop</defName>
                            <label>random grains seeds drop</label>
                            <description>Call for a drop of 100 random grains seeds.\n - Contains: rice, wheat, corn, beans.\n\nPermit points cost - 1 point.</description>
                            <minTitle>Acolyte</minTitle>
                            <uiPosition>(0,28)</uiPosition>
                            <royalAid>
                                <itemsToDrop>
                                    <Seed_Potato>100</Seed_Potato>
                                </itemsToDrop>
                                <favorCost>4</favorCost>
                            </royalAid>
                        </RoyalTitlePermitDef>
                        <OrderedStuffDef ParentName="OrderedSeedsBase">
                            <defName>SeedsPotatoDropStuff</defName>
                            <thingsToChoose>
                                <li>Seed_Rice</li>
                                <li>Seedwheat</li>
                                <li>Seed_Corn</li>
                                <li>Seedbean</li>
                            </thingsToChoose>
                        </OrderedStuffDef>
                        
                        <RoyalTitlePermitDef ParentName="DropResourcesPlus">
                            <defName>SeedsRiceDrop</defName>
                            <label>random vegetables seeds drop</label>
                            <description>Call for a drop of 100 random vegetables seeds.\n - Contains: potato, carrot, tomato.\n\nPermit points cost - 1 point.</description>
                            <minTitle>Acolyte</minTitle>
                            <uiPosition>(1,28)</uiPosition>
                            <royalAid>
                                <itemsToDrop>
                                    <Seed_Rice>100</Seed_Rice>
                                </itemsToDrop>
                                <favorCost>4</favorCost>
                            </royalAid>
                        </RoyalTitlePermitDef>
                        <OrderedStuffDef ParentName="OrderedSeedsBase">
                            <defName>SeedsRiceDropStuff</defName>
                            <thingsToChoose>
                                <li>Seed_Potato</li>
                                <li>SeedCarrot</li>
                                <li>SeedTomato</li>
                            </thingsToChoose>
                        </OrderedStuffDef>
                        
                        <RoyalTitlePermitDef ParentName="DropResourcesPlus">
                            <defName>SeedsOakDrop</defName>
                            <label>random tree seeds drop</label>
                            <description>Call for a drop of 30 random tree seeds.\n\nPermit points cost - 2 point.</description>
                            <minTitle>Acolyte</minTitle>
                            <uiPosition>(0,29)</uiPosition>
                            <permitPointCost>2</permitPointCost>
                            <royalAid>
                                <itemsToDrop>
                                    <Seed_TreeOak>30</Seed_TreeOak>
                                </itemsToDrop>
                                <favorCost>4</favorCost>
                            </royalAid>
                        </RoyalTitlePermitDef>
                        <OrderedStuffDef ParentName="OrderedSeedsBase">
                            <defName>SeedsOakDropStuff</defName>
                            <thingsToChoose>
                                <li>Seed_TreeDrago</li>
                                <li>Seed_TreeOak</li>
                                <li>Seed_TreeWillow</li>
                                <li>Seed_TreeCypress</li>
                                <li>Seed_TreeMaple</li>
                                <li>Seed_TreePoplar</li>
                                <li>Seed_TreePine</li>
                                <li>Seed_TreeBirch</li>
                                <li>Seed_TreeTeak</li>
                                <li>Seed_TreeCecropia</li>
                                <li>Seed_TreePalm</li>
                                <li>Seed_TreeBamboo</li>
                                <li>SeedHeveaTree</li>
                                <li>Seed_Redwood</li>
                            </thingsToChoose>
                        </OrderedStuffDef>
                        
                        <RoyalTitlePermitDef ParentName="DropResourcesPlus">
                            <defName>SeedsAppleDrop</defName>
                            <label>random fruit tree seeds drop</label>
                            <description>Call for a drop of 15 random fruit tree seeds.\n - Contains: apple, banana, orange and peach trees.\n\nPermit points cost - 2 point.</description>
                            <minTitle>Acolyte</minTitle>
                            <uiPosition>(1,29)</uiPosition>
                            <permitPointCost>2</permitPointCost>
                            <royalAid>
                                <itemsToDrop>
                                    <SeedAppletree>15</SeedAppletree>
                                </itemsToDrop>
                                <favorCost>4</favorCost>
                            </royalAid>
                        </RoyalTitlePermitDef>
                        <OrderedStuffDef ParentName="OrderedSeedsBase">
                            <defName>SeedsAppleDropStuff</defName>
                            <thingsToChoose>
                                <li>SeedAppletree</li>
                                <li>SeedBananaTree</li>
                                <li>SeedorangeTree</li>
                                <li>SeedPeachTree</li>
                            </thingsToChoose>
                        </OrderedStuffDef>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>