<?xml version="1.0" encoding="UTF-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[LTS]Furnishing</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>Defs/ArchitectSense.DesignationSubCategoryDef[defName="Columns"]/defNames</xpath>
            <value>
                <li>LightColumnMod</li>
                <li>OrbitalTradeColumnMod</li>
                <li>SunColumnMod</li>
                <li>IceColumnMod</li>
                <li>GraveColumnMod</li>
            </value>
        </match>
        <nomatch Class="PatchOperationAdd">
            <xpath>Defs</xpath>
            <value>
                <ArchitectSense.DesignationSubCategoryDef>
                    <defName>Columns</defName>
                    <label>Columns</label>
                    <description>Various Columns.</description>
                    <defNames>
                        <li>Column</li>
                        <li>LightColumnMod</li>
                        <li>OrbitalTradeColumnMod</li>
                        <li>SunColumnMod</li>
                        <li>IceColumnMod</li>
                        <li>GraveColumnMod</li>
                    </defNames>
                    <designationCategory>Structure</designationCategory>
                    <debug>false</debug>
                </ArchitectSense.DesignationSubCategoryDef>
            </value>
        </nomatch>
    </Operation>
</Patch>